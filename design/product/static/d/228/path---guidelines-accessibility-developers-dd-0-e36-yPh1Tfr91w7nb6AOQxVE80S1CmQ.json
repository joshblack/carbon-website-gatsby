es":{},"children":[{"type":"text","value":"aria-labelledby"}]},{"type":"text","value":" along with "},{"type":"element","tagName":"code","properties":{},"children":[{"type":"text","value":"id"}]},{"type":"text","value":" attributes instead of "},{"type":"element","tagName":"code","properties":{},"children":[{"type":"text","value":"alt"}]},{"type":"text","value":" text. If the image is decorative, use an empty "},{"type":"element","tagName":"code","properties":{},"children":[{"type":"text","value":"alt"}]},{"type":"text","value":" attribute, otherwise the screen reader will read the whole image url if the "},{"type":"element","tagName":"code","properties":{},"children":[{"type":"text","value":"alt"}]},{"type":"text","value":" is left out."}]},{"type":"text","value":"\n"},{"type":"element","tagName":"pre","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-html"]},"children":[{"type":"text","value":"<!-- Example 1 -->\n<img src='puppy.jpg' title='Sleeping Puppy' alt='A sleeping puppy'/>\n"}]}]},{"type":"text","value":"\n"},{"type":"element","tagName":"pre","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-html"]},"children":[{"type":"text","value":"<!-- Example 2 -->\n<img src='puppy2.jpg' aria-labelledby='imagelabel'/>\n<p id='imagelabel'>This is a picture of a cute puppy in cup</p>\n"}]}]},{"type":"text","value":"\n"},{"type":"element","tagName":"h3","properties":{"id":"audio-alternatives"},"children":[{"type":"element","tagName":"a","properties":{"href":"#audio-alternatives","ariaHidden":true,"className":["anchor"]},"children":[{"type":"element","tagName":"svg","properties":{"height":16,"width":16,"fill":"#0062ff","xmlns":"http://www.w3.org/2000/svg","viewBox":"0 0 32 32"},"children":[{"type":"element","tagName":"defs","properties":{},"children":[{"type":"element","tagName":"style","properties":{},"children":[{"type":"text","value":".cls-1{fill:none;}"}]}]},{"type":"el